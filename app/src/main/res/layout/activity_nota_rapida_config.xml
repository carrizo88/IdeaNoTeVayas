<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="iocompany.ideanotevayas.Activities.NotaRapidaConfig">

<include android:id="@+id/toolbarNotaRapidaConfigActivity"
    layout="@layout/toolbar_1"/>

    <Switch
        android:id="@+id/switchNotaRapidaConfig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nota Rapida"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbarNotaRapidaConfigActivity"
        android:textSize="25dp"/>

    <Button
        android:id="@+id/buttonNotaRapidaConfig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/switchNotaRapidaConfig"
        android:layout_marginTop="15dp"
        android:text="Configurar" />
    <!--
    <ListView
        android:id="@+id/ListViewNotaRapidaConfig"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbarNotaRapidaConfigActivity"
        />
    -->



</RelativeLayout>

